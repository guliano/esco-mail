{
    "name": "guliano/esco-mail",
    "description": "ZF2 module wrapping mail functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "zend",
        "mail"
    ],
    "require": {
        "php": "^5.6|^7.0,<7.4",
        "zendframework/zend-modulemanager": "~2.4",
        "zendframework/zend-mail": "~2.4",
        "zendframework/zend-view": "~2.4",
        "zendframework/zend-log": "~2.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0",
        "mikey179/vfsstream": "~1.4",
        "zendframework/zend-mvc": "~2.4",
        "zendframework/zend-uri": "~2.4",
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^2.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "EscoMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EscoMailTest\\": "test/"
        }
    }
}
