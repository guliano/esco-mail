{
    "name": "guliano/esco-mail",
    "description": "Library provides extra layer for zf2 mail functions",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "zend",
        "mail"
    ],
    "require": {
        "php": ">=5.3.0",
        "zendframework/zend-modulemanager": "~2.3",
        "zendframework/zend-mail": "~2.3",
        "zendframework/zend-view": "~2.3",
        "zendframework/zend-log": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~2.0",
        "mikey179/vfsStream": "~1.4",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-uri": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "EscoMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EscoMailTest\\": "test/"
        }
    }
}